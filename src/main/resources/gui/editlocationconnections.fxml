<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sepm.dsa.gui.EditLocationConnectionsController">
   <left>
      <BorderPane prefHeight="200.0" prefWidth="380.0" BorderPane.alignment="CENTER">
         <center>
            <TableView fx:id="suggestLocationConnectionsTable" prefHeight="371.0" prefWidth="351.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="scConnectedToColumn" prefWidth="231.0" text="Reiseverbindung zu" />
                <TableColumn fx:id="scTravelTimeColumn" prefWidth="102.0" text="Reisedauer" />
              </columns>
            </TableView>
         </center>
         <top>
            <FlowPane alignment="BASELINE_CENTER" prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#onSuggestConnectionsClicked" text="Vorschläge">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </Button>
                  <Separator orientation="VERTICAL" prefWidth="10.0" />
                  <Button mnemonicParsing="false" onAction="#onFilterConnectionsClicked" text="Filter">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </Button>
                  <TextField fx:id="locationNameFilter" prefHeight="39.0" prefWidth="127.0" promptText="Ortname">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </TextField>
               </children>
            </FlowPane>
         </top>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </BorderPane>
   </left>
   <top>
      <Label text="Reiseverbindungen für Ort verwalten" BorderPane.alignment="CENTER">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <bottom>
      <FlowPane alignment="CENTER_RIGHT" prefHeight="50.0" BorderPane.alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#onFinishedClicked" text="Fertig" />
         </children>
      </FlowPane>
   </bottom>
   <right>
      <BorderPane prefHeight="200.0" prefWidth="380.0" BorderPane.alignment="CENTER">
         <center>
            <TableView fx:id="locationConnectionsTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <columns>
                  <TableColumn fx:id="connectionToColumn" prefWidth="229.0" text="Reiseverbindung zu" />
                  <TableColumn fx:id="travelTimeColumn" minWidth="0.0" prefWidth="122.0" text="Reisedauer" />
               </columns>
            </TableView>
         </center>
         <top>
            <FlowPane alignment="TOP_CENTER" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" onAction="#onEditConnectionClicked" text="bearbeiten">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin></Button>
                  <Button mnemonicParsing="false" onAction="#onRemoveConnectionClicked" text="entfernen">
                     <FlowPane.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </FlowPane.margin>
                  </Button>
               </children>
            </FlowPane>
         </top>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </BorderPane>
   </right>
   <center>
      <Button mnemonicParsing="false" onAction="#onApplyAllClicked" text="alle =&gt;" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sepm.dsa.gui.MainMenuController">
<top><Pane prefHeight="608.0" prefWidth="800.0" BorderPane.alignment="CENTER">
<children>
        <MenuBar fx:id="menuBar" prefHeight="25.0" prefWidth="800.0" BorderPane.alignment="CENTER">
            <menus>
                <Menu fx:id="dateiMenu" mnemonicParsing="false" text="Datei">
                    <items>
                        <MenuItem fx:id="dateiImport" disable="true" mnemonicParsing="false" text="Import" />
                        <MenuItem fx:id="dateiExport" disable="true" mnemonicParsing="false" text="Export" />
                        <MenuItem fx:id="dateiExit" mnemonicParsing="false" onAction="#onExitClicked" text="Beenden" />
                    </items>
                </Menu>
                <Menu fx:id="dateiVerwaltenMenu" mnemonicParsing="false" text="Daten verwalten">
                    <items><Menu fx:id="verwaltenWeltkarte" mnemonicParsing="false" text="Weltkarte">

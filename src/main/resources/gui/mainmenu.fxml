    <MenuItem fx:id="weltkarteImportieren" mnemonicParsing="false" onAction="#onWeltkarteImportierenPressed" text="Importieren" /><MenuItem fx:id="weltkarteExportieren" mnemonicParsing="false" onAction="#onWeltkarteExportierenPressed" text="Exportieren" />
  </items>
</Menu>
                        <MenuItem fx:id="verwaltenHaendlerKategorie" mnemonicParsing="false" onAction="#onTraderCategoriesClicked" text="Händlerkategorie" />
                        <MenuItem fx:id="verwaltenGebieteGrenzen" mnemonicParsing="false" onAction="#onGrenzenGebieteClicked" text="Gebiete &amp; Grenzen" />
                        <MenuItem fx:id="verwaltenWaehrungen" disable="true" mnemonicParsing="false" text="Währungen" />
                        <MenuItem fx:id="verwaltenWaren" disable="true" mnemonicParsing="false" text="Waren" />
                        <MenuItem mnemonicParsing="false" onAction="#onShowLocationsClicked" text="Orte" /><MenuItem mnemonicParsing="false" onAction="#onTradersPressed" text="Händlerverwaltung" />
                    </items>
                </Menu>
            </menus>
        </MenuBar><ImageView fx:id="worldMapImageView" fitHeight="550.0" fitWidth="400.0" layoutX="14.0" layoutY="36.0" pickOnBounds="true" preserveRatio="true" />
</children></Pane>
</top>
</BorderPane>
